{
    "compilerOptions": {
        "target": "ES2020",
        "module": "CommonJS",
        "lib": [
            "ES2020"
        ],
        "skipLibCheck": true,
        "strict": false,
        "esModuleInterop": true,
        "noEmit": false,
        "noImplicitAny": false,
        "noUnusedLocals": false,
        "noUnusedParameters": false
    },
    "include": [
        "*.ts"
    ],
    "exclude": [
        "node_modules"
    ]
}